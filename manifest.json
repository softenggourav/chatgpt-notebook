{
    "manifest_version": 3,
    "name": "chatGPT-notebook",
    "version": "1.0",
    "description": "A simple Chrome extension to read chat titles from chat.openai.com",
    "permissions": [
      "activeTab",
      "scripting",
      "storage",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://chat.openai.com/*"],
        "js": ["content.js"]
      }   
    ],
    "host_permissions": [
        "https://*.openai.com/*"
      ]
  }
  